{
  "name": "rpg",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.7.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "2.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
